<body>
  <%- include('partials/_header') %>
    <h2>Create Account</h2>
    <form method="POST" action="/register">
      <h5>Email address</h5>
      <input class="form-control" type="text" name="email" style="width: 300px;">
      <h5>Password</h5>
      <input class="form-control" type="password" name="master_password" style="width: 300px;">
      <h5>Organization ID</h5>
      <input class="form-control" type="number" name="organization_id" style="width: 300px;">
      <br>
      <button type="submit" class="btn-primary" value="submit">Register</button>
    </form>
</body>

